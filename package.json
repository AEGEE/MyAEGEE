{
  "name": "core",
  "version": "1.22.2",
  "description": "The core module of my.aegee.eu",
  "keywords": [],
  "author": "Sergey Peshkov",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "codecov": "^3.6.5",
    "conventional-changelog": "^3.1.18",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.18.0",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.2.11",
    "nock": "^12.0.3",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "semantic-release": "^17.1.1"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run db:setup && jest test/**/*.test.js --runInBand --forceExit",
    "lint": "node_modules/.bin/eslint .",
    "lint:fix": "node_modules/.bin/eslint --fix .",
    "lint-staged": "lint-staged",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate; rm -f state/.seed-executed-$NODE_ENV",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "LOGLEVEL=info node --experimental-repl-await scripts/seed.js && touch state/.seed-executed-$NODE_ENV",
    "db:clear": "LOGLEVEL=info node  --experimental-repl-await scripts/clear.js; rm -f state/.seed-executed-$NODE_ENV",
    "cli": "node --experimental-repl-await -r ./cli.js",
    "open-coverage": "open-cli coverage/lcov-report/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "middlewares/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js",
      "!lib/fs.js",
      "!lib/merge.js",
      "!lib/errors.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "dependencies": {
    "@bugsnag/js": "^6.5.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "express-query-boolean": "^2.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "on-finished": "^2.3.0",
    "pg": "^8.2.1",
    "prom-client": "^12.0.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.1"
  }
}
