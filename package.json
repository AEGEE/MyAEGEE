{
  "name": "oms-core-js",
  "version": "0.1.0",
  "description": "The core module of my.aegee.eu",
  "keywords": [],
  "author": "Sergey Peshkov",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "codecov": "^3.6.5",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.18.0",
    "faker": "^4.1.0",
    "husky": "^3.0.4",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "nyc": "^15.0.0",
    "open-cli": "^5.0.0",
    "request-promise-native": "^1.0.8"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run db:setup && jest test/**/*.test.js --runInBand --forceExit",
    "lint": "node_modules/.bin/eslint .",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "cli": "node --experimental-repl-await -r ./cli.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "open-coverage": "open-cli coverage/lcov-report/index.html"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "middlewares/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js",
      "!lib/fs.js",
      "!lib/merge.js",
      "!lib/errors.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "dependencies": {
    "@bugsnag/js": "^6.5.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-query-boolean": "^2.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "pg": "^7.17.1",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1"
  }
}
